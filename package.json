{
  "name": "@actabl-pdesign/bellhop-ui",
  "version": "1.1.1",
  "description": "React UI components library with Tailwind CSS",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actabl-pdesign/bellhop-ui.git"
  },
  "author": "actabl-pdesign",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c && npm run build:css",
    "build:css": "tailwindcss -i ./src/styles/globals.css -o ./dist/styles.css --minify",
    "dev": "rollup -c -w",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "prettier": "prettier --write src/**/*.{ts,tsx}",
    "prettier:check": "prettier --check src/**/*.{ts,tsx}",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:a11y": "jest --testPathPattern=a11y",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky install",
    "semantic-release": "semantic-release"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "tailwindcss": ">=3.0.0",
    "@tailwindcss/forms": ">=0.5.0"
