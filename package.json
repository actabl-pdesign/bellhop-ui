{
  "name": "@actabl-pdesign/bellhop-ui",
  "version": "1.0.0",
  "description": "Bellhop styled React components.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./styles": "./dist/style.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "vite build --config vite.config.lib.ts",
    "build:watch": "vite build --config vite.config.lib.ts --watch",
    "storybook": "storybook dev -p 6006 --no-open",
    "build-storybook": "storybook build -o ./storybook-static",
    "lint": "eslint components --fix",
    "lint:check": "eslint components",
    "prettier": "prettier . --write",
    "prettier:check": "prettier . --check",
    "type-check": "tsc --noEmit",
    "test": "pnpm run type-check && pnpm run lint:check && pnpm run prettier:check",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "react",
    "components",
    "tailwind",
    "ui",
    "design-system",
    "typescript"
